{
    "name": "myfmbutler/myfmapilibrary-for-php",
    "type": "library",
    "description": "(Claris) FileMaker 19 API PHP wrapper",
    "Keywords": ["Lesterius", "FileMaker", "Claris", "API", "REST", "Database", "Wrapper"],
    "license": "gpl-3.0",
    "version": "3.0.0",
    "minimum-stability": "stable",
    "authors": [
      {
        "name": "Lesterius",
        "email": "infoFR@lesterius.com",
        "homepage": "https://www.lesterius.com"
      }
